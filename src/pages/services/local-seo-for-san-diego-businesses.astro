---

<Layout title={pageTitle} description={pageDescription}>
	<Header />

	<!-- Hero Section -->
	<section class="relative min-h-[70vh] flex items-center bg-cover bg-center"
		style={`background-image: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)), url('${hero.backgroundImage}');`}>
		<div class="container mx-auto px-4 sm:px-6 lg:px-8 relative z-10">
			<div class="max-w-4xl">
				<h1 class="text-6xl md:text-7xl lg:text-8xl font-bold text-white mb-6 uppercase" style="font-family: 'Bebas Neue', sans-serif; letter-spacing: 0.05em;">
					{hero.title}<br/><span class="text-orange-600">{hero.titleHighlight}</span>
				</h1>
				<div class="w-24 h-1 bg-orange-600 mb-8"></div>
				<p class="text-xl md:text-2xl text-gray-200 mb-12 leading-relaxed">
					{hero.description}
				</p>
				<div class="flex flex-wrap gap-4">
					<a href={hero.primaryButton.link} class="btn-primary px-8 py-4 text-lg font-bold uppercase tracking-wider border-2 transition-all hover:bg-white hover:text-orange-600">
						{hero.primaryButton.text}
					</a>
					<a href={hero.secondaryButton.link} class="bg-transparent border-2 border-white text-white px-8 py-4 text-lg font-bold uppercase tracking-wider transition-all hover:bg-white hover:text-black">
						{hero.secondaryButton.text}
					</a>
				</div>
			</div>
		</div>
	</section>

	<!-- Trust Signals -->
	<TrustSignals
		title="Why Choose Glassbee Solutions"
		subtitle="Proven results that drive real business growth"
		stats={trustStats}
	/>

	<!-- Intro Section -->
	<section class="py-20 bg-white">
		<div class="container mx-auto px-4 sm:px-6 lg:px-8">
			<div class="max-w-5xl mx-auto">
				<div class="card p-8 md:p-12 bg-gray-50 border-l-4 border-orange-600">
					<div class="text-center mb-8">
						<h2 class="text-3xl md:text-4xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">
							{introSection.title}<br/><span class="text-orange-600">{introSection.titleHighlight}</span>
						</h2>
						<div class="w-24 h-1 bg-orange-600 mx-auto mb-4"></div>
						<p class="text-xl md:text-2xl font-semibold text-gray-800 mb-8">
							{introSection.subtitle}
						</p>
					</div>

					<div class="space-y-6 text-left">
						{introSection.paragraphs.map((paragraph) => (
							<p class="text-base md:text-lg text-gray-700 leading-relaxed">
								{paragraph}
							</p>
						))}
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- Local SEO That Drives Real Results Section -->
	<section class="py-20 bg-gray-50">
		<div class="container mx-auto px-4 sm:px-6 lg:px-8">
			<div class="max-w-4xl mx-auto">
				<div class="text-center mb-12">
					<h2 class="text-3xl md:text-4xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">
						{localSEOSection.title}<br/><span class="text-orange-600">{localSEOSection.titleHighlight}</span>
					</h2>
					<div class="w-24 h-1 bg-orange-600 mx-auto mb-8"></div>
					<p class="text-lg md:text-xl text-gray-700 leading-relaxed mb-8">
						{localSEOSection.description}
					</p>
				</div>

				<!-- Keyword Tags Display -->
				<div class="flex flex-wrap justify-center gap-3 mb-8">
					{localSEOSection.keywords.map((keyword) => (
						<span class="bg-orange-600 text-white px-4 py-2 text-sm md:text-base font-semibold uppercase tracking-wide">
							{keyword}
						</span>
					))}
				</div>

				<p class="text-base md:text-lg text-gray-600 text-center leading-relaxed">
					{localSEOSection.keywordsDescription}
