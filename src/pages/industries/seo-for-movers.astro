										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"></path>
									</svg>
								)}
								{feature.icon === "settings" && (
									<svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 4a2 2 0 114 0v1a1 1 0 001 1h3a1 1 0 011 1v3a1 1 0 01-1 1h-1a2 2 0 100 4h1a1 1 0 011 1v3a1 1 0 01-1 1h-3a1 1 0 01-1-1v-1a2 2 0 10-4 0v1a1 1 0 01-1 1H7a1 1 0 01-1-1v-3a1 1 0 00-1-1H4a2 2 0 110-4h1a1 1 0 001-1V7a1 1 0 011-1h3a1 1 0 001-1V4z"></path>
									</svg>
								)}
								{feature.icon === "document" && (
									<svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
									</svg>
								)}
								{feature.icon === "chart" && (
									<svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"></path>
									</svg>
								)}
							</div>
							<h3 class="text-xl md:text-2xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">{feature.heading}</h3>
							<p class="text-base md:text-lg text-gray-600 leading-relaxed">
								{feature.description}
							</p>
						</div>
					)
				))}
			</div>
		</div>
	</section>

	<!-- Why Our Approach Works Faster Section -->
	<section class="py-20 bg-white">
		<div class="container mx-auto px-4 sm:px-6 lg:px-8">
			<div class="max-w-4xl mx-auto text-center">
				<h2 class="text-3xl md:text-4xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">
					{whyWorksSection.title}<br/><span class="text-orange-600">{whyWorksSection.titleHighlight}</span>
				</h2>
				<div class="w-24 h-1 bg-orange-600 mx-auto mb-8"></div>
				<p class="text-lg md:text-xl text-gray-600 leading-relaxed">
					{whyWorksSection.text}
				</p>
			</div>
		</div>
	</section>

	<!-- How It Works Section -->
	<section class="py-20 bg-gray-50">
		<div class="container mx-auto px-4 sm:px-6 lg:px-8">
			<div class="text-center mb-16">
				<h2 class="text-3xl md:text-4xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">
					{howItWorksSection.title}
				</h2>
				<div class="w-24 h-1 bg-orange-600 mx-auto mb-6"></div>
			</div>

			<div class="max-w-4xl mx-auto">
				<div class="space-y-8">
					{howItWorksSection.steps.map((step) => (
						step.heading && (
							<div class="flex gap-6">
								<div class="flex-shrink-0">
									<div class="w-16 h-16 bg-orange-600 flex items-center justify-center text-white text-3xl font-bold" style="font-family: 'Bebas Neue', sans-serif;">{step.number}</div>
								</div>
								<div>
									<h3 class="text-xl md:text-2xl font-bold text-black mb-3 uppercase" style="font-family: 'Bebas Neue', sans-serif;">{step.heading}</h3>
									<p class="text-base md:text-lg text-gray-600 leading-relaxed">
										{step.description}
									</p>
								</div>
							</div>
						)
					))}
				</div>
			</div>
		</div>
	</section>

	<!-- Emergency Availability Section -->
	<section class="py-20 bg-white">
		<div class="container mx-auto px-4 sm:px-6 lg:px-8">
			<div class="max-w-5xl mx-auto">
				<div class="bg-gradient-to-br from-orange-600 to-orange-700 rounded-none p-12 md:p-16 text-white relative overflow-hidden">
					<div class="absolute top-0 right-0 w-64 h-64 bg-white opacity-5 rounded-full -mr-32 -mt-32"></div>
					<div class="absolute bottom-0 left-0 w-48 h-48 bg-white opacity-5 rounded-full -ml-24 -mb-24"></div>

					<div class="relative z-10 text-center">
						<svg class="w-20 h-20 text-white mx-auto mb-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>
						</svg>

						<h2 class="text-4xl md:text-5xl font-bold mb-6 uppercase" style="font-family: 'Bebas Neue', sans-serif;">
							{emergencySection.title}<br/><span class="opacity-90">{emergencySection.titleHighlight}</span>
						</h2>

						<p class="text-lg md:text-xl leading-relaxed max-w-3xl mx-auto text-white/95">
							{emergencySection.text}
						</p>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- Service Areas Section -->
	<section class="py-20 bg-gray-50">
		<div class="container mx-auto px-4 sm:px-6 lg:px-8">
			<div class="max-w-5xl mx-auto">
				<div class="text-center mb-12">
					<h2 class="text-3xl md:text-4xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">
						Service <span class="text-orange-600">Area</span>
					</h2>
					<div class="w-24 h-1 bg-orange-600 mx-auto mb-6"></div>
					<p class="text-lg md:text-xl text-gray-600 mb-8">
						We provide Moving Company SEO services for:
					</p>
				</div>

				<div class="grid grid-cols-2 md:grid-cols-3 gap-4 mb-8">
					{serviceAreas.map((area) => (
						<div class="card p-6 text-center hover:shadow-lg transition-shadow">
							<svg class="w-8 h-8 text-orange-600 mx-auto mb-3" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
							</svg>
							<p class="font-bold text-black text-sm md:text-base">{area}</p>
						</div>
					))}
				</div>

				<div class="text-center">
					<p class="text-base md:text-lg text-gray-600 mb-4">
						Serving:
					</p>
					<p class="text-sm md:text-base text-gray-500">
						Local and long-distance moving companies • Residential, commercial, and storage movers
					</p>
				</div>
			</div>
		</div>
	</section>

	<!-- Related Services -->
	<RelatedPages
		title="Related Services"
		pages={relatedServices}
	/>

	<!-- FAQ Section -->
	<FAQ
		title="Frequently Asked Questions"
		subtitle="Everything you need to know about moving company SEO"
		faqs={faqs.filter(faq => faq.question)}
	/>

	<!-- CTA Banner -->
	<CTABanner
		backgroundColor={ctaBanner.backgroundColor}
		title={ctaBanner.title}
		subtitle={ctaBanner.subtitle}
		primaryButtonText={ctaBanner.primaryButton.text}
		primaryButtonLink={ctaBanner.primaryButton.link}
		secondaryButtonText={ctaBanner.secondaryButton.text}
		secondaryButtonLink={ctaBanner.secondaryButton.link}
	/>

	<Footer />
</Layout>

