---

<Layout title={pageTitle} description={pageDescription} keywords={pageKeywords}>
	<Header />

	<!-- Schema.org JSON-LD -->
	<script type="application/ld+json" set:html={JSON.stringify({
		"@context": "https://schema.org",
		"@type": "Service",
		"serviceType": "Med Spa SEO Services",
		"name": "Med Spa SEO Services San Diego",
		"description": pageDescription,
		"provider": {
			"@type": "LocalBusiness",
			"name": "Glassbee Solutions",
			"address": {
				"@type": "PostalAddress",
				"addressLocality": "San Diego",
				"addressRegion": "CA",
				"addressCountry": "US"
			},
			"telephone": "+16192733218",
			"email": "info@glassbeesolutions.org"
		},
		"areaServed": {
			"@type": "City",
			"name": "San Diego"
		}
	})} />

	<!-- Hero Section -->
	<section class="relative min-h-[70vh] flex items-center bg-cover bg-center"
		style={`background-image: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)), url('${hero.backgroundImage}');`}>
