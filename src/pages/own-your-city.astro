---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import TrustSignals from '../components/TrustSignals.astro';
import CTABanner from '../components/CTABanner.astro';

// SEO & Page Info
const pageTitle = "OWN YOUR CITY™ System | 45-Day Results | 60-Day Guarantee | San Diego";
const pageDescription = "The only local domination system designed specifically for San Diego home service companies. See results in 45 days — backed by a 60-day domination guarantee. Exclusively for San Diego home service businesses.";

// Hero Section
