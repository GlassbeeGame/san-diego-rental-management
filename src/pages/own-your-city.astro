	title: "OWN YOUR CITY™",
	titleHighlight: "SYSTEM",
	subtitle: "See Results in 45 Days — Backed by a 60-Day Domination Guarantee",
	description: "Exclusively for San Diego Home Service Businesses",
	backgroundImage: "https://images.unsplash.com/photo-1559136555-9303baea8ebd?w=1920",
	primaryButton: {
		text: "Claim Your Founding Member Spot",
		link: "/contact"
	},
	secondaryButton: {
		text: "Call (619) 273-3218",
		link: "tel:+16192733218"
	}
};

// Trust Signals Stats
const trustStats: Array<{
	value: string;
	label: string;
	icon: 'properties' | 'rating' | 'revenue' | 'years' | 'occupancy';
}> = [
	{ value: "45 Days", label: "To Results", icon: "properties" },
	{ value: "60 Day", label: "Money-Back Guarantee", icon: "revenue" },
	{ value: "$56,500", label: "Real-World Value", icon: "rating" },
	{ value: "3 Spots", label: "Founding Members Left", icon: "occupancy" }
];
