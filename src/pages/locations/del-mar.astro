---
import Layout from '../../layouts/Layout.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';
import TrustSignals from '../../components/TrustSignals.astro';
import RelatedPages from '../../components/RelatedPages.astro';
import FAQ from '../../components/FAQ.astro';
import CTABanner from '../../components/CTABanner.astro';

// SEO & Page Info
const pageTitle = "Del Mar SEO Experts ‚Äî Local Visibility for High-Value Clients | Glassbee Solutions";
const pageDescription = "Attract more leads with strategic SEO for Del Mar & North County. From beachfront restaurants to high-end real estate teams, we help Del Mar businesses dominate search results.";

// Schema Markup for Local Business SEO
const schemaMarkup = {
	"@context": "https://schema.org",
	"@type": "ProfessionalService",
	"name": "Glassbee Solutions - Del Mar SEO",
	"description": "Local SEO services for Del Mar businesses. We help Del Mar businesses dominate search results and attract high-value clients through strategic SEO campaigns.",
	"url": "https://www.glassbeesolutions.com/locations/del-mar",
	"areaServed": {
		"@type": "City",
		"name": "Del Mar",
		"containedIn": {
			"@type": "City",
			"name": "San Diego",
			"containedIn": "California"
		}
	},
	"serviceArea": [
		"Del Mar",
		"Del Mar Village",
		"Beach Colony",
		"Del Mar Heights",
		"Crest Canyon",
		"Carmel Valley",
		"Torrey Hills",
		"Solana Beach",
		"Encinitas"
	],
	"priceRange": "$$",
	"telephone": "+1-951-760-0070",
	"address": {
		"@type": "PostalAddress",
		"addressRegion": "CA",
		"addressLocality": "San Diego"
	}
};

// Hero Section
const hero = {
	title: "Del Mar SEO Experts",
	titleHighlight: "Local Visibility for High-Value Clients",
	description: "Attract More Leads With Strategic SEO for Del Mar & North County",
	backgroundImage: "https://images.unsplash.com/photo-1559827260-dc66d52bef19?w=1920",
	primaryButton: {
		text: "Get Free SEO Audit",
		link: "/contact"
	},
	secondaryButton: {
		text: "Call (951) 760-0070",
		link: "tel:+19517600070"
	}
};

// Trust Signals Stats
const trustStats = [
	{ value: "200%+", label: "Search Growth", icon: "properties" },
	{ value: "Top 3", label: "Google Maps Ranking", icon: "revenue" },
	{ value: "2 Weeks", label: "Full Setup", icon: "occupancy" },
	{ value: "24 Hours", label: "Response Time", icon: "rating" }
];

// About Section (Two Column Layout)
const aboutSection = {
	title: "Del Mar SEO Experts",
	titleHighlight: "Local Visibility for High-Value Clients",
	leftColumn: {
		heading: "SEO That's Local, Intelligent, and Built to Convert",
		description: "Del Mar is small but ultra-competitive ‚Äî your potential clients have options everywhere from Solana Beach to Carmel Valley. You need SEO that's local, intelligent, and built to convert.",
		bullets: [
			"Deep Local Knowledge: We specialize in Del Mar & North County SEO, tailoring every campaign to the local search landscape and seasonal trends",
			"AI-Driven Strategy: Our proprietary AI tools pinpoint profitable, low-competition keywords unique to the Del Mar market",
			"60-Day Money-Back Guarantee: Our proven 45-day system delivers results. If you don't see measurable growth within 60 days, we'll refund every penny",
			"Two-Week Setup Timeline: Full campaign setup and optimization are typically complete within two weeks, with continuous improvements every month"
		]
	},
	rightColumn: {
		heading: "The Del Mar SEO Landscape",
		description: "Del Mar's blend of luxury coastal living and tourism makes it one of the most competitive pockets of San Diego SEO. From boutique hotels to home-service providers, everyone's fighting for the top 3 spots in Google Maps.",
		revenueSeasons: [
			{ season: "What Our Clients See", price: "200%+ growth", occupancy: "In local search impressions" },
			{ season: "Google Maps Rankings", price: "Top-3 placement", occupancy: "For high-intent queries" },
			{ season: "Lead Generation", price: "Increased calls & forms", occupancy: "From residents & visitors" }
		]
	}
};

// Black Box Section (Service Areas)
const blackBoxSection = {
	title: "Local Service",
	titleHighlight: "Areas We Cover",
	items: [
		{
			heading: "Del Mar Village",
			description: "Restaurants, boutiques, galleries, and local businesses in the heart of Del Mar"
		},
		{
			heading: "Beach Colony & 15th Street",
			description: "Hospitality, vacation rentals, and beachfront businesses"
		},
		{
			heading: "Del Mar Heights & Crest Canyon",
			description: "Home service contractors, real estate teams, and professional services"
		},
		{
			heading: "Carmel Valley & Torrey Hills",
			description: "Professional services, med spas, and corporate offices"
		},
		{
			heading: "Surrounding Cities",
			description: "We also serve Solana Beach, Encinitas, and La Jolla"
		},
		{
			heading: "Response Time & Meetings",
			description: "Within 24 hours for all Del Mar clients. We often meet clients near Del Mar Plaza, Camino del Mar, or the Del Mar Racetrack area."
		}
	]
};

// Expertise/Service Cards
const expertiseCards = [
	{
		heading: "Real Estate Teams & Brokerages",
		description: "Dominate luxury real estate searches in Del Mar and the coastal corridor. We optimize for high-value queries like \"Del Mar homes for sale,\" \"luxury real estate Del Mar,\" and neighborhood-specific searches that attract qualified buyers."
	},
	{
		heading: "Med Spas & Aesthetic Clinics",
		description: "Stand out in Del Mar's competitive wellness market. We target affluent clients searching for \"med spa Del Mar,\" \"aesthetic treatments Carmel Valley,\" and premium beauty services."
	},
	{
		heading: "Coastal Home-Service Providers",
		description: "Get more calls for roofing, landscaping, cleaning, HVAC, and other home services. We optimize for Del Mar Heights, Crest Canyon, and Carmel Valley searches that drive booked jobs."
	},
	{
		heading: "Local Hospitality Brands",
		description: "Fill tables and book reservations for restaurants, vacation rentals, salons, and other hospitality businesses. We target both locals and tourists searching for Del Mar experiences."
	},
	{
		heading: "Professional Offices",
		description: "Attorneys, consultants, designers, and other professional services ‚Äî we help you rank for high-intent local searches and capture quality leads in Del Mar and North County."
	},
	{
		heading: "Businesses Near Major Employers",
		description: "We also support organizations connected to The Del Mar Fairgrounds, Scripps Clinic Torrey Pines, and UC San Diego Health La Jolla."
	}
];

// Related Pages
const relatedPages = [
	{
		title: "Local SEO for San Diego Businesses",
		description: "Complete SEO management for your business",
		link: "/services/local-seo-for-san-diego-businesses",
		icon: "services"
	},
	{
		title: "Google Business SEO",
		description: "GBP optimization and management services",
		link: "/services/google-business-seo",
		icon: "services"
	},
	{
		title: "View All Service Areas",
		description: "See all San Diego locations we serve",
		link: "/locations",
		icon: "contact"
	}
];

// Nearby Locations
const nearbyLocations = [
	{ name: "Solana Beach", description: "Coastal cliffs and village SEO", href: "/locations/solana-beach" },
	{ name: "Encinitas", description: "Surf culture and wellness SEO", href: "/locations/encinitas" },
	{ name: "La Jolla", description: "Luxury coastal community SEO", href: "/locations/la-jolla" },
	{ name: "Carlsbad", description: "North County coastal SEO", href: "/locations/carlsbad" }
];

// FAQ Section
const faqs = [
	{
		question: "How quickly can I see results from Del Mar SEO services?",
		answer: "Most clients see measurable traction within 30 days, including improved rankings and increased local search impressions. Full campaign setup is typically complete within two weeks, with continuous improvements every month. You can expect 200%+ growth in local search impressions and Top-3 Google Maps rankings for high-intent queries."
	},
	{
		question: "What makes your Del Mar SEO different from other agencies?",
		answer: "We're 100% focused on Del Mar and North County with deep knowledge of the local search landscape and seasonal trends. Our proprietary AI tools pinpoint profitable, low-competition keywords unique to the Del Mar market. Plus, we offer a 60-day money-back guarantee ‚Äî our proven 45-day system delivers results, or we'll refund every penny."
	},
	{
		question: "Do you optimize for specific Del Mar neighborhoods?",
		answer: "Yes! We create custom content and keyword strategies for Del Mar Village, Beach Colony, 15th Street area, Del Mar Heights, Crest Canyon, Carmel Valley, and Torrey Hills. Each area has unique search patterns and customer demographics that require tailored approaches."
	},
	{
		question: "What industries do you specialize in for Del Mar?",
		answer: "We specialize in real estate teams and brokerages, med spas and aesthetic clinics, coastal home-service providers (roofing, landscaping, cleaning), local hospitality brands (restaurants, vacation rentals, salons), and professional offices (attorneys, consultants, designers)."
	},
	{
		question: "How do Del Mar events and tourism affect SEO?",
		answer: 'Del Mar\'s weather, tourism, and event cycles (like the Del Mar Races and county fair) directly impact online searches. We build SEO strategies that adapt to tourist season spikes ("best restaurant Del Mar"), home-service upticks during coastal storm seasons, and luxury market trends for spas, real estate, and design firms.'
	}
];

// CTA Banner
const ctaBanner = {
	title: "Start Ranking in Del Mar Now",
	subtitle: "Let's make your business the top result for Del Mar SEO and local searches that convert. üìç Serving Del Mar, Solana Beach, Carmel Valley, and the greater North County area.",
	primaryButton: {
		text: "Schedule Free SEO Audit",
		link: "/contact"
	},
	secondaryButton: {
		text: "Call (951) 760-0070",
		link: "tel:+19517600070"
	}
};
---

<Layout title={pageTitle} description={pageDescription}>
	<!-- Schema Markup -->
	<script type="application/ld+json" set:html={JSON.stringify(schemaMarkup)} />

	<Header />

	<!-- Hero Section -->
	<section class="relative min-h-[70vh] flex items-center bg-cover bg-center"
		style={`background-image: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)), url('${hero.backgroundImage}');`}>
		<div class="container mx-auto px-4 sm:px-6 lg:px-8 relative z-10">
			<div class="max-w-4xl">
				<h1 class="text-6xl md:text-7xl lg:text-8xl font-bold text-white mb-6 uppercase" style="font-family: 'Bebas Neue', sans-serif; letter-spacing: 0.05em;">
					{hero.title}<br/><span class="text-orange-600">{hero.titleHighlight}</span>
				</h1>
				<div class="w-24 h-1 bg-orange-600 mb-8"></div>
				<p class="text-xl md:text-2xl text-gray-200 mb-12 leading-relaxed">
					{hero.description}
				</p>
				<div class="flex flex-wrap gap-4">
					<a href={hero.primaryButton.link} class="btn-primary px-8 py-4 text-lg font-bold uppercase tracking-wider border-2 transition-all hover:bg-white hover:text-orange-600">
						{hero.primaryButton.text}
					</a>
					<a href={hero.secondaryButton.link} class="bg-transparent border-2 border-white text-white px-8 py-4 text-lg font-bold uppercase tracking-wider transition-all hover:bg-white hover:text-black">
						{hero.secondaryButton.text}
					</a>
				</div>
			</div>
		</div>
	</section>

	<!-- Trust Signals -->
	<TrustSignals stats={trustStats} />

	<!-- About Section -->
	<section class="py-20 bg-white">
		<div class="container mx-auto px-4 sm:px-6 lg:px-8">
			<div class="max-w-6xl mx-auto">
				<div class="text-center mb-16">
					<h2 class="text-5xl md:text-6xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">
						{aboutSection.title}<br/><span class="text-orange-600">{aboutSection.titleHighlight}</span>
					</h2>
					<div class="w-24 h-1 bg-orange-600 mx-auto mb-6"></div>
					<p class="text-xl text-gray-600 max-w-3xl mx-auto">
						At Glassbee Solutions, we help Del Mar businesses dominate search results across San Diego's coastal corridor. From beachfront restaurants to high-end real-estate teams, our Del Mar SEO services are built to increase visibility, traffic, and conversions ‚Äî turning Google searches into paying clients.
					</p>
					<p class="text-lg text-gray-600 max-w-3xl mx-auto mt-4">
						Whether you're a contractor serving the hills above the racetrack or a med spa on Camino del Mar, our goal is simple: make your business the one locals see first.
					</p>
				</div>

				<div class="grid grid-cols-1 md:grid-cols-2 gap-12 mb-16">
					<!-- Left Column -->
					<div>
						<h3 class="text-3xl font-bold text-black mb-6 uppercase" style="font-family: 'Bebas Neue', sans-serif;">{aboutSection.leftColumn.heading}</h3>
						<p class="text-gray-600 mb-6 leading-relaxed">
							{aboutSection.leftColumn.description}
						</p>
						<ul class="space-y-4">
							{aboutSection.leftColumn.bullets.map((bullet) => (
								<li class="flex items-start gap-3">
									<svg class="w-6 h-6 text-orange-600 flex-shrink-0 mt-1" fill="currentColor" viewBox="0 0 20 20">
										<path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
									</svg>
									<span class="text-gray-700">{bullet}</span>
								</li>
							))}
						</ul>
					</div>

					<!-- Right Column -->
					<div>
						<h3 class="text-3xl font-bold text-black mb-6 uppercase" style="font-family: 'Bebas Neue', sans-serif;">{aboutSection.rightColumn.heading}</h3>
						<p class="text-gray-600 mb-6 leading-relaxed">
							{aboutSection.rightColumn.description}
						</p>
						<div class="space-y-4">
							{aboutSection.rightColumn.revenueSeasons.map((season) => (
								<div class="bg-gray-50 p-4 border-l-4 border-orange-600">
									<p class="font-bold text-black mb-1">{season.season}</p>
									<p class="text-gray-600 text-sm">{season.price} | {season.occupancy}</p>
								</div>
							))}
						</div>
					</div>
				</div>

				<!-- Black Box Section -->
				<div class="bg-black text-white p-12 mb-16">
					<h3 class="text-4xl font-bold mb-8 uppercase text-center" style="font-family: 'Bebas Neue', sans-serif;">
						{blackBoxSection.title}<br/><span class="text-orange-600">{blackBoxSection.titleHighlight}</span>
					</h3>
					<div class="grid grid-cols-1 md:grid-cols-3 gap-8">
						{blackBoxSection.items.map((item) => (
							<div>
								<h4 class="text-xl font-bold mb-3 uppercase" style="font-family: 'Bebas Neue', sans-serif;">{item.heading}</h4>
								<p class="text-gray-300 text-sm">{item.description}</p>
							</div>
						))}
					</div>
				</div>

				<!-- Driving Directions -->
				<div class="mb-16">
					<h3 class="text-4xl font-bold text-black mb-6 uppercase text-center" style="font-family: 'Bebas Neue', sans-serif;">
						Driving Directions &<br/><span class="text-orange-600">Local Meetings</span>
					</h3>
					<div class="card p-8">
						<p class="text-gray-600 text-lg leading-relaxed">
							We often meet clients near Del Mar Plaza, Camino del Mar, or the Del Mar Racetrack area. Whether you're based in Carmel Valley, La Jolla, or Solana Beach, our team is minutes away ‚Äî or available instantly via Zoom.
						</p>
					</div>
				</div>

				<!-- Local Insights -->
				<div class="mb-16">
					<h3 class="text-4xl font-bold text-black mb-6 uppercase text-center" style="font-family: 'Bebas Neue', sans-serif;">
						Local Insights &<br/><span class="text-orange-600">Seasonal Search Patterns</span>
					</h3>
					<div class="card p-8">
						<p class="text-gray-600 text-lg leading-relaxed mb-4">
							Del Mar's weather, tourism, and event cycles (like the Del Mar Races and county fair) directly impact online searches. We build SEO strategies that adapt to:
						</p>
						<ul class="space-y-3">
							<li class="flex items-start gap-3">
								<svg class="w-6 h-6 text-orange-600 flex-shrink-0 mt-1" fill="currentColor" viewBox="0 0 20 20">
									<path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
								</svg>
								<span class="text-gray-700">Tourist season spikes ("best restaurant Del Mar," "luxury rentals near Del Mar Beach")</span>
							</li>
							<li class="flex items-start gap-3">
								<svg class="w-6 h-6 text-orange-600 flex-shrink-0 mt-1" fill="currentColor" viewBox="0 0 20 20">
									<path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
								</svg>
								<span class="text-gray-700">Home-service upticks during coastal storm seasons</span>
							</li>
							<li class="flex items-start gap-3">
								<svg class="w-6 h-6 text-orange-600 flex-shrink-0 mt-1" fill="currentColor" viewBox="0 0 20 20">
									<path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
								</svg>
								<span class="text-gray-700">Luxury market trends for spas, real estate, and design firms</span>
							</li>
						</ul>
					</div>
				</div>

				<!-- Expertise Cards -->
				<div class="mb-16">
					<h3 class="text-4xl font-bold text-black mb-8 uppercase text-center" style="font-family: 'Bebas Neue', sans-serif;">
						Industries<br/><span class="text-orange-600">We Serve in Del Mar</span>
					</h3>
					<div class="grid grid-cols-1 md:grid-cols-2 gap-8">
						{expertiseCards.map((card) => (
							<div class="card p-8">
								<h4 class="text-2xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">{card.heading}</h4>
								<p class="text-gray-600 mb-4">{card.description}</p>
							</div>
						))}
					</div>
				</div>

				<!-- Comparison Table -->
				<div class="mb-16">
					<h3 class="text-4xl font-bold text-black mb-8 uppercase text-center" style="font-family: 'Bebas Neue', sans-serif;">
						How Glassbee Solutions<br/><span class="text-orange-600">Stands Apart</span>
					</h3>
					<div class="overflow-x-auto">
						<table class="w-full card">
							<thead class="bg-gray-50">
								<tr>
									<th class="px-6 py-4 text-left font-bold text-black">Feature</th>
									<th class="px-6 py-4 text-center font-bold text-orange-600">Glassbee Solutions</th>
									<th class="px-6 py-4 text-center font-bold text-gray-600">Typical SEO Agency</th>
								</tr>
							</thead>
							<tbody class="divide-y divide-gray-200">
								<tr>
									<td class="px-6 py-4 text-gray-700">Hyper-Local Precision</td>
									<td class="px-6 py-4 text-center text-sm">Built on real Del Mar search data and competitor analysis</td>
									<td class="px-6 py-4 text-center text-gray-500 text-sm">Generalized San Diego targeting</td>
								</tr>
								<tr class="bg-gray-50">
									<td class="px-6 py-4 text-gray-700">Data + AI Strategy</td>
									<td class="px-6 py-4 text-center text-sm">Combines machine learning insights with human optimization</td>
									<td class="px-6 py-4 text-center text-gray-500 text-sm">One-size-fits-all templates</td>
								</tr>
								<tr>
									<td class="px-6 py-4 text-gray-700">Full Optimization</td>
									<td class="px-6 py-4 text-center text-sm">On-page, off-page, citations, and Google Business handled for you</td>
									<td class="px-6 py-4 text-center text-gray-500 text-sm">Partial implementation</td>
								</tr>
								<tr class="bg-gray-50">
									<td class="px-6 py-4 text-gray-700">Results-Driven Focus</td>
									<td class="px-6 py-4 text-center text-sm">+200% growth in calls & web traffic, tracked transparently</td>
									<td class="px-6 py-4 text-center text-gray-500 text-sm">Vanity metrics and impressions</td>
								</tr>
								<tr>
									<td class="px-6 py-4 text-gray-700">True Partnership</td>
									<td class="px-6 py-4 text-center text-sm">Direct communication, transparent progress, real accountability</td>
									<td class="px-6 py-4 text-center text-gray-500 text-sm">Automated monthly reports only</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- Related Services -->
	<RelatedPages
		title="Related Services"
		pages={relatedPages}
	/>

	<!-- Nearby Locations -->
	{nearbyLocations && nearbyLocations.length > 0 && (
		<section class="py-20 bg-gray-50">
			<div class="container mx-auto px-4 sm:px-6 lg:px-8">
				<div class="text-center mb-12">
					<h2 class="text-4xl md:text-5xl font-bold text-black mb-4 uppercase" style="font-family: 'Bebas Neue', sans-serif;">
						We Also Serve<br/><span class="text-orange-600">Nearby Communities</span>
					</h2>
					<div class="w-24 h-1 bg-orange-600 mx-auto"></div>
				</div>
				<div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6 max-w-6xl mx-auto">
					{nearbyLocations.map((location) => (
						<a href={location.href} class="card p-6 hover:border-orange-600 transition-all group">
							<h3 class="text-xl font-bold text-black mb-2 uppercase group-hover:text-orange-600" style="font-family: 'Bebas Neue', sans-serif;">{location.name}</h3>
							<p class="text-gray-600 text-sm">{location.description}</p>
						</a>
					))}
				</div>
			</div>
		</section>
	)}

	<!-- FAQ Section -->
	<FAQ
		title="Frequently Asked Questions"
		subtitle="About Del Mar SEO"
		faqs={faqs}
	/>

	<!-- CTA Banner -->
	<CTABanner
		backgroundColor="orange"
		title={ctaBanner.title}
		subtitle={ctaBanner.subtitle}
		primaryButtonText={ctaBanner.primaryButton.text}
		primaryButtonLink={ctaBanner.primaryButton.link}
		secondaryButtonText={ctaBanner.secondaryButton.text}
		secondaryButtonLink={ctaBanner.secondaryButton.link}
	/>

	<Footer />
</Layout>
